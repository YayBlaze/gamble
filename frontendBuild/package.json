		"svelte-check": "^4.0.0",
		"svelte-confetti": "^2.3.1",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.8",
		"dotenv": "^16.6.1",
		"gasp": "^0.0.2",
		"gsap": "^3.13.0",
		"tailwindcss": "^4.1.8",
		"watch": "^1.0.2"
	}
